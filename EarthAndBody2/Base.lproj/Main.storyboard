                                            <imageView clipsSubviews="YES" userInteractionEnabled="NO" contentMode="scaleAspectFit" horizontalHuggingPriority="251" verticalHuggingPriority="251" fixedFrame="YES" image="spinach-artichoke-lasagna-recipe" translatesAutoresizingMaskIntoConstraints="NO" id="eMv-MR-Gym">
